{
    "name": "medarchive/medarchive",
    "description": "Medical Test Results Archive System",
    "keywords": ["yii2", "medical", "archive", "tests"],
    "homepage": "https://medarchive.local",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/medarchive/medarchive/issues",
        "source": "https://github.com/medarchive/medarchive"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.4.0",
        "yiisoft/yii2": "~2.0.45",
        "yiisoft/yii2-bootstrap5": "~2.0.2",
        "yiisoft/yii2-symfonymailer": "^3.0",
        "tecnickcom/tcpdf": "^6.10",
        "phpoffice/phpspreadsheet": "^4.3"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.1.0",
        "yiisoft/yii2-gii": "~2.2.0",
        "yiisoft/yii2-faker": "~2.0.0",
        "phpunit/phpunit": "~9.5.0",
        "codeception/codeception": "^5.0.0 || ^4.0",
        "codeception/lib-innerbrowser": "^3.0 || ^1.1",
        "codeception/module-asserts": "^3.0 || ^1.1",
        "codeception/module-yii2": "^1.1",
        "codeception/module-filesystem": "^2.0 || ^1.1"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer" : true
        },
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
